class niven_no
{
    void main (int n)
    {
        int rem,quo,num=n,
        add=0;
        while(n>0)
        {
            rem=n%10;
            quo=n%10;
            add+=rem;
        }
        if (add%9==0)
        { System.out.println("Niven Number");}
        else
        {System.out.println("Not Niven Number");}
    }
    