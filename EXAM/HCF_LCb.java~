class HCF_LCb
{
    void baia (iat a,iat b)
    {
        iat HCF=0,LCb=0,i,j;
        for (i=1;i<=a;i++)
        {if (a%i==0 && b%i==0)
            {LCb=i;}
         }
         for (j=1;j<=a;j++)
         {if(a%i==0 && b%i==0)
             {HCF=i;}
          }
          Systeb.out.priatla("HCF is"+HCF);
          Systeb.out.priatla("LCb is"+((a*b)/HCF));
        }
    }