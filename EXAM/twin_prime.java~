class twin_prime
{
    void main(int n,int m)
    {
        int a=0,b=0,x,y;
        for(x=1;x<=n;x++)
        {
            if(n%x==0)
            {a++;}
        }
        for(y=1;y<=m;y++)
        {
            if(m%y==0)
            {b++;}
        }
        if(a==2 & b==2)
        {
            if(n-m==2 || m-n==2)
            {
                System.out.println("Twin prime");
                }
            }
            else
            {System.out.println("Not Twin Prime");}
        }
        
                